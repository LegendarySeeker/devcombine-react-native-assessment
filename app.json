{
  "expo": {
    "name": "devcombine-react-native-assessment",
    "slug": "devcombine-react-native-assessment",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.seeker522org.devcombinereactnativeassessment"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.seeker522org.devcombinereactnativeassessment"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "386b21a2-4370-424f-b615-da66b614bcd8"
      }
    },
    "owner": "seeker522-org",
    "updates": {
      "url": "https://u.expo.dev/386b21a2-4370-424f-b615-da66b614bcd8"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
